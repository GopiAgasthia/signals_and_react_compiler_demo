{
  "name": "react-signals-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:useState": "vite --config vite.useState-implementation.config.js",
    "dev:signals": "vite --config vite.signals-implementation.config.js",
    "dev:all": "concurrently --kill-others --names \"USESTATE,SIGNALS\" --prefix \"[{name}]\" --prefix-colors \"blue.bold,green.bold\" \"npm run dev:useState\" \"npm run dev:signals\"",
    "build": "vite build",
    "build:useState": "vite build --config vite.useState-implementation.config.js",
    "build:signals": "vite build --config vite.signals-implementation.config.js",
    "build:all": "concurrently --names \"USESTATE,SIGNALS\" --prefix \"[{name}]\" --prefix-colors \"blue.bold,green.bold\" \"npm run build:useState\" \"npm run build:signals\"",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:useState": "vite preview --config vite.useState-implementation.config.js",
    "preview:signals": "vite preview --config vite.signals-implementation.config.js"
  },
  "dependencies": {
    "@preact/signals-react": "^2.3.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "concurrently": "^8.2.2",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.1.0"
  }
}
